#include <stdio.h>
#include<locale.h>

int main() {
    int g1, g2;
    setlocale(LC_ALL,"portuguese");
    printf("Digite o número de gols do primeiro time: ");
    scanf("%d", &g1);
    printf("Digite o número de gols do segundo time: ");
    scanf("%d", &g2);

    if (g1 > g2) {
        printf("Vitória do primeiro time\n");
    } else if (g1 < g2) {
        printf("Vitória do segundo time\n");
    } else {
        printf("Empate\n");
    }
}
    
